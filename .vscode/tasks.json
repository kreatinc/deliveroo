{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "go develop",
      "type": "shell",
      "command": "git checkout develop"
    },
    {
      "label": "refresh",
      "type": "shell",
      "command": "git pull origin develop"
    },
    { "label": "get dependencies", "type": "shell", "command": "yarn" },
    {
      "label": "start dev",
      "type": "shell",
      "command": "yarn start",
      "runOptions": {
        "runOn": "folderOpen"
      }
    },
    {
      "label": "Init",
      "type": "shell",
      "dependsOrder": "sequence",
      "dependsOn": ["go develop", "start dev"],
      "runOptions": {
        "runOn": "folderOpen"
      }
    }
  ]
}
